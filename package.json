{
  "name": "stride",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --project=unit --coverage --reporter=junit --outputFile=test-report.junit.xml --reporter=default && vitest run --project=storybook --coverage --coverage.reportsDirectory=./coverage-storybook --reporter=junit --outputFile=test-report-storybook.junit.xml --reporter=default",
    "test:ui": "vitest --ui",
    "test:e2e": "npx playwright test",
    "test:e2e:coverage": "PLAYWRIGHT_COVERAGE=true npm run build && PLAYWRIGHT_COVERAGE=true npx playwright test && nyc report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/roboto": "^5.2.10",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@tanstack/react-query": "^5.90.21",
    "arktype": "^2.1.29",
    "axios": "^1.13.6",
    "chart.js": "^4.5.1",
    "chartjs-plugin-zoom": "^2.2.0",
    "framer-motion": "^12.34.3",
    "jszip": "^3.10.1",
    "konva": "^10.2.0",
    "leaflet": "^1.9.4",
    "luxon": "^3.7.2",
    "mapbox-gl": "^3.19.0",
    "ol": "^10.8.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-konva": "^19.2.2",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.13.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@chromatic-com/storybook": "^5.0.1",
    "@codecov/vite-plugin": "^1.9.1",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-docs": "^10.2.13",
    "@storybook/addon-vitest": "^10.2.13",
    "@storybook/react": "^10.2.12",
    "@storybook/react-vite": "^10.2.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/leaflet": "^1.9.21",
    "@types/luxon": "^3.7.1",
    "@types/mapbox-gl": "^3.5.0",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "baseline-browser-mapping": "^2.10.0",
    "chromatic": "^15.2.0",
    "jsdom": "^28.1.0",
    "nyc": "^18.0.0",
    "playwright": "^1.58.2",
    "storybook": "^10.2.13",
    "typescript": "~5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-svgr": "^4.5.0",
